    "version": "0.2.0",
    "description": "th Fjplayer html5 video player",
    "main": "lib/fjplayer.js",
    "scripts": {
        "build": "webpack --mode=build",
        "dev": "webpack --progress --colors --watch --mode=dev",
        "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.spec.js",
        "start": " concurrently \"npm run dev\" \"npm run live\" ",
        "web": "webpack-dev-server --progress --colors",
        "live": "http-server -a localhost -p 8000 -c-1"
    },
