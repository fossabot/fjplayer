        "babel-preset-es2015": "6.3.13",
        "chai": "3.4.1",
        "eslint": "1.7.2",
        "eslint-loader": "1.1.0",
        "mocha": "2.3.4",
        "webpack": "1.12.9",
        "yargs": "3.32.0",
        "http-server": "^0.9.0",
        "css-loader": "0.26.1",
        "style-loader": "0.13.1",
        "file-loader": "^0.8.1",
        "html-loader": "^0.4.4",
