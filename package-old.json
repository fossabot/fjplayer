{
    "scripts": {
        "dev": "webpack  --color --config webpack.config.js  -d",
        "devw": "webpack  --color --config webpack.config.js  -d --watch",
        "testPlayer": "NODE_ENV=testing karma run  -- --grep=FjTestPlayer",
        "web": "http-server -a localhost -p 8000 -c-1 ",
